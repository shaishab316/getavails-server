{
  "template": {
    "name": "express-it",
    "repository": "https://github.com/shaishab316/express-it.git"
  },
  "name": "getavails",
  "version": "1.9.0",
  "description": "A comprehensive event management system with multi-role support including artists, agents, venues, and organizers. Features real-time chat, Stripe payments, booking management, and detailed analytics dashboards.",
  "main": "dist/server.js",
  "author": {
    "name": "Shaishab Chandra Shil",
    "github": "https://github.com/shaishab316"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaishab316/getavails-server.git"
  },
  "scripts": {
    "prepare": "simple-git-hooks && npx dotenvx ext precommit --install",
    "new-module": "tsx ./src/app/modules/.builder.ts",
    "build": "tsx esbuild.mts",
    "start": "npx dotenvx run -- node dist/server.js",
    "dev": "npm run type-check && set DEBUG=socket.io* && npx dotenvx run -- tsx --watch --inspect src/server.ts",
    "dev-f5": "concurrently npm:dev npm:prisma-studio",
    "seed-admin": "npx dotenvx run -- tsx scripts/seedAdmin.ts",
    "seed-stripe": "npx dotenvx run -- tsx scripts/seedStripe.ts",
    "lint": "eslint src --ignore-path .gitignore --ext .ts",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma-studio": "npx dotenvx run -- npx prisma studio",
    "prisma-reset": "npx dotenvx run -- npx prisma migrate reset --force",
    "prisma-migrate": "npx dotenvx run -- npx prisma migrate dev",
    "prisma-generate": "rm -rf prisma && npx prisma generate",
    "prisma-seed": "npx dotenvx run -- npx prisma db seed",
    "env-encrypt": "npx dotenvx encrypt -f .env",
    "env-decrypt": "npx dotenvx decrypt -f .env"
  },
  "keywords": [
    "getavails",
    "driver",
    "management",
    "platform"
  ],
  "license": "MIT",
  "simple-git-hooks": {
    "pre-commit": "npm run lint && npm run type-check && npm run prettier"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mjml": "^4.7.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.16",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.11",
    "esbuild-node-externals": "^1.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "inquirer": "^12.10.0",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "prisma": "^6.18.0",
    "semantic-release": "^24.2.7",
    "simple-git-hooks": "^2.13.1",
    "table": "^6.9.0",
    "tsx": "^4.20.6",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@types/node": "^24.10.1",
    "axios": "^1.12.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "chalk": "^5.6.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "handlebars": "^4.7.8",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mjml": "^4.16.1",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.7",
    "ora": "^9.0.0",
    "otplib": "^12.0.1",
    "redis": "^5.9.0",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.1",
    "strip-ansi": "^7.1.2",
    "stripe": "^19.1.0",
    "winston": "^3.13.0",
    "zod": "^4.1.12"
  }
}
